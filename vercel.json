{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "memory": 1024,
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "dest": "/api/index.py"
    },
    {
      "src": "/docs.html",
      "dest": "/api/index.py"
    },
    {
      "src": "/redoc",
      "dest": "/api/index.py"
    },
    {
      "src": "/openapi.json",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/stats",
      "dest": "/api/index.py"
    },
    {
      "src": "/analyze",
      "dest": "/api/index.py"
    },
    {
      "src": "/kol/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/kols",
      "dest": "/api/index.py"
    },
    {
      "src": "/compare",
      "dest": "/api/index.py"
    },
    {
      "src": "/cache/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/history/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "kol-analyzer/src:kol-analyzer",
    "DEBUG": "1"
  }
}
